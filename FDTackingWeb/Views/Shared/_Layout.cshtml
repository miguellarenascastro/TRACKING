    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/SemanticJs")

    <script>
        $('#main-sidebar')
            .sidebar({ context: $("#wrapper"), transition: 'overlay' })
            .sidebar('attach events', '#main-sidebar-trigger');

        $(document).ready(function () {
            $('.ui.dropdown').dropdown({
                fullTextSearch: true
            });

            $('body').on('submit', 'form', function (e) {
                var form = $(this);
                var buttons = form.find('.ui.button');
                buttons.addClass('loading submitted');
                buttons.prop('disabled');
                buttons.attr('disabled');
            });

            var dataTableOptions2 = {
                language: {
                    url: "//cdn.datatables.net/plug-ins/1.10.19/i18n/Spanish.json"
                },
                lengthMenu: [[10, 20, 40, 6, 0, 80, 100, -1], [10, 20, 40, 60, 80, 100, "Todos"]]

            };



            $(".datatable").DataTable(dataTableOptions2);

            $('.ui.dropdown').dropdown();
        });
    </script>

    @RenderSection("scripts", required: false)
